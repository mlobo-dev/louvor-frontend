ol-md-12\">\n              <label htmlFor=\"inputNome\">Pesquisar musicas</label>\n              <input\n                className=\"form-control mr-sm-2\"\n                type=\"search\"\n                placeholder=\"Começe a digitar o nome de uma música ou cantor/artista/ministério \"\n                aria-label=\"Search\"\n                onChange={(e) => atualizarOpcoes(e.target.value)}\n              />\n            </div>\n          </div>\n\n          <div className=\"row\">\n            <div className=\"pick-list-area\"></div>\n\n            <PickList\n              opcoes={musicas}\n              acaoOpcoes={selecionarMusica}\n              acaoSelecionadas={atribuirMusicasSelecionadas}\n              opcoesTitle={'Disponíveis'}\n              selecionadas={musicasSelecionadas}\n              selecionadasTitle={'Selecionadas'}\n            />\n          </div>\n\n          <div className=\"buttons-group\">\n            <button\n              type=\"button\"\n              className=\"btn btn-secondary mr-2 btn-cadastro\"\n              onClick={salvar}\n            >\n              Salvar\n            </button>\n\n            <Link\n              className=\"btn btn-secondary mr-2 btn-cadastro\"\n              to={'/musicas'}\n            >\n              Voltar\n            </Link>\n          </div>\n        </form>\n      </>\n    );\n  }\n\n  return (\n    <>\n      {!useSelector((state) => state.usuarioLogado) && <Redirect to=\"/\" />}\n      <Navbar />\n      <div>\n        <Container title=\"Cadastro de Repertório\" content={formCadastro()} />\n      </div>\n    </>\n  );\n}\nexport default CadastroRepertorio;\n","/home/mlobo/Desktop/Workspaces/WolfLabs/louvor-frontend/src/components/PickList/index.js",["87","88","89","90"],"import React, { useEffect } from 'react';\nimport { Link } from 'react-router-dom';\nimport * as musicaService from '../../services/musicaService';\nimport './picklist.css';\n\nfunction PickList({\n  opcoes,\n  opcoesTitle,\n  s